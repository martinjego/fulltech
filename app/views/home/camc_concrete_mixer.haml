= render 'shared/header' 

%section.jumbotron.d-flex.flex-column.text-center.pt-0.pb-0.position-relative.no-border-bottom#hero{"style": "background-image: url('assets/aboutus-bg.png/');"}
  .container.h-100.position-relative
    = render 'navbar'
    .hero-content
      %h3.text-uppercase.text-light.text-left.text-bold Trucks
      %h1.text-uppercase.text-light.text-left.mr-4.mt-4 Built for this.
      %p.text-light.text-left
        Lorem Ipsum

= render 'list'

%section#our-products.mb-4.pb-5
  .container
    .row.pt-4
      .col-md-12
        %ul.nav.nav-pills
          %li.active
            %button.btn.btn-danger.text-light.mr-2{"data-toggle": "pill", href: "#content-1"} CAMC 10m³ Concrete Mixer

    .tab-content
      #content-1.tab-pane.fade.show.active
        .row.pt-4
          .col-md-12
            %h3 CAMC 10m³ Concrete Mixer
            %h3
              %span.text-bold Model:
              %span HN3252A31C6M4
        .row
          .col-md-12
            = image_tag 'trucks/camc/A2.png', class: 'w-100'
        .row.pt-4
          .col-md-6
            .description-container
              %h4.w-100.border-bottom.pb-3
                %span.text-red.text-bold Engine:
                %span.text-red CM6D18 345 40
                %span.text-black.ml-4 345HP
              %p.pb-4
                %span.text-bold Type:
                %span.pull-right Four-stroke, water-cooled, in-line, wet-liner, direct-injection 
                %span.pull-right with <span class="text-red text-bold">ENGINE BRAKE, fuel economy controller </span>
              %p
                %span.text-bold Max. Power:
                %span.pull-right 254kw / 1900rpm
              %p
                %span.text-bold Max. Torque:
                %span.pull-right 1660N.M / 1100~1400rpm
              %p
                %span.text-bold No. of cylinder:
                %span.pull-right 6
              %p
                %span.text-bold Bore x Stroke:
                %span.pull-right 118 * 150mm
              %p
                %span.text-bold Displacement:
                %span.pull-right 9.84L
              %p
                %span.text-bold Controller type:
                %span.pull-right Electronic
              %p
                %span.text-bold Air cleaner:
                %span.pull-right Dry paper element
              %p.pb-5
                %span.text-bold Emission Standard:
                %span.pull-right Euro
            .description-container
              %h4.w-100.border-bottom.pb-3
                %span.text-red.text-bold Transmission
              %p
                %span.text-bold Model:
                %span.pull-right 9SDB2051A
              %p
                %span.text-bold Brand:
                %span.pull-right CAMC (same level as ZF technology)
              %p.pb-3
                %span.text-bold Type:
                %span.pull-right Manual gear box with <span class="text-red text-bold"> Synchronizer, servo &amp; Creep </span>
              %p
                %span.text-bold Ratio:
                .row
                  .col-md-6
                    %p
                      %span 1<sup>st</sup>:
                      %span.pull-right 12.07
                    %p
                      %span 2<sup>nd</sup>:
                      %span.pull-right 8.35
                    %p
                      %span 3<sup>rd</sup>:
                      %span.pull-right 5.67
                    %p
                      %span 4<sup>th</sup>:
                      %span.pull-right 4.07
                  .col-md-6
                    %p
                      %span 5<sup>th</sup>:
                      %span.pull-right 2.96
                    %p
                      %span 6<sup>th</sup>:
                      %span.pull-right 2.05
                    %p
                      %span 7<sup>th</sup>:
